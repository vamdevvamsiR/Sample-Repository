#include<iostream>
#include<conio.h>
using namespace std;
int main()
{
	int n, Arr[n],Ele,i;
	cout<<"Enter the size of the array:";
	cin>>n;
	cout<<"Enter the elements: \n";
	for(i=0;i<n;i++)
	{
		cin>>Ele;
		Arr[i]=Ele;
	}
	cout<<"The Array given is:";
	for(i=0;i<n;i++)
	{
		cout<<Arr[i]<<" ";
	}
}
